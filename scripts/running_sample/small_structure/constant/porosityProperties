FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      porosityProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

D 0;
F 220; //170 good
X 1e7;//1e10;
W 1e10;
// -x means multiplying
// 1000 tooo much, to low
// 500 still too much
// 250?

porousBlockage_x1
{
    type            DarcyForchheimer;

    cellZone        porousBlockage_x1;



    d   ($D $X $X);
    f   ($F $X $X);

    coordinateSystem
    {
        type    cartesian;
        origin (0 0 0);
        //origin  (1.22 1.14 0);
        coordinateRotation
        {
            type    axesRotation;
            //e1  (1 0 0);
            //e2  (0 1 0);
            e2      (0 1 0);
            e3      (-1 0 2);
        }
    }
}

